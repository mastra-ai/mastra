{
  "name": "@mastra/inbox-github",
  "version": "0.1.0",
  "description": "GitHub inbox adapter for Mastra - sync GitHub issues as agent tasks",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@octokit/webhooks": "^13.4.3"
  },
  "peerDependencies": {
    "@mastra/core": "workspace:*",
    "@octokit/rest": "^21.1.1"
  },
  "devDependencies": {
    "@internal/types-builder": "workspace:*",
    "@mastra/core": "workspace:*",
    "@octokit/rest": "^21.1.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mastra",
    "inbox",
    "github",
    "issues",
    "tasks",
    "agent"
  ],
  "license": "Elastic-2.0"
}
