{
  "defaultLocale": "en",
  "files": {
    "mdx": {
      "include": [
        "src/content/**/*.mdx"
      ],
      "transform": [
        {
          "match": "^src/content/{locale}/docs/(.*)\\.mdx$",
          "replace": "i18n/{locale}/docusaurus-plugin-content-docs/current/$1.mdx"
        },
        {
          "match": "^src/content/{locale}/(examples|guides|models|reference)/(.*)\\.mdx$",
          "replace": "i18n/{locale}/docusaurus-plugin-content-docs-$1/current/$2.mdx"
        }
      ]
    },
    "json": {
      "include": [
        "i18n/[locale]/**/*.json"
      ]
    },
    "gt": {
      "output": "i18n/locales/[locale].json"
    }
  },
  "locales": [
    "ja"
  ],
  "options": {
    "experimentalClearLocaleDirs": true,
    "experimentalLocalizeStaticUrls": true,
    "experimentalAddHeaderAnchorIds": "mintlify"
  },
  "projectId": "48751091-773e-4a3e-8c83-5c3906117b30",
  "_versionId": "914a45cac81dd86ef28e841ec15db2e72236dabeb2a42df1421d7d983d53bc9c"
}