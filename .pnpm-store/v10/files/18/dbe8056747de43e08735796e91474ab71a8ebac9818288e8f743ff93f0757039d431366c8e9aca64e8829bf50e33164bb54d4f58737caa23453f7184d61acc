            };
          case "json_delta":
            return {
              type: "json_delta",
              data: event.data
            };
          case "error":
            return {
              type: "error",
              data: JSON.parse(event.data)
            };
          case "progress":
            return {
              type: "progress",
              data: SSEProgressEventData.parse(JSON.parse(event.data))
            };
          case "console":
            return {
              type: "console",
              data: SSEConsoleEventData.parse(JSON.parse(event.data))
            };
          case "start":
            return {
              type: "start",
              data: ""
            };
          case "done":
            return {
              type: "done",
              data: ""
            };
          default: {
            const _event = event;
            throw new Error(`Unknown event type ${JSON.stringify(_event)}`);
          }
        }
      }
