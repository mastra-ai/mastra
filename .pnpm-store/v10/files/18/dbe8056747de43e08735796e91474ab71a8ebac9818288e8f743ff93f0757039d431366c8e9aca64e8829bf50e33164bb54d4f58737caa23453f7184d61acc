// dev/stream.ts
function serializeSSEEvent(event) {
  return Object.entries(event).filter(([_key, value]) => value !== void 0).map(([key, value]) => `${key}: ${value}`).join("\n") + "\n\n";
}

// dev/types.ts
init_generated_types();

var evalBodySchema = _v3.z.object({
