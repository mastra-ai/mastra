// src/AutoForm.tsx
var import_jsx_runtime4 = require("react/jsx-runtime");
function AutoForm({
  schema,
  onSubmit = () => {
  },
  defaultValues,
  values,
  children,
  uiComponents,
  formComponents,
  withSubmit = false,
  onFormInit = () => {
  },
  formProps = {}
}) {
  const parsedSchema = (0, import_core4.parseSchema)(schema);
  const methods = (0, import_react_hook_form3.useForm)({
    defaultValues: {
      ...(0, import_core4.getDefaultValues)(schema),
      ...defaultValues
    },
    values
  });
  (0, import_react2.useEffect)(() => {
    if (onFormInit) {
      onFormInit(methods);
    }
  }, [onFormInit, methods]);
  const handleSubmit = async (dataRaw) => {
    const data = (0, import_core4.removeEmptyValues)(dataRaw);
    const validationResult = schema.validateSchema(data);
    console.log("validationResult", { validationResult, dataRaw, data });
    if (validationResult.success) {
      await onSubmit(validationResult.data, methods);
    } else {
      methods.clearErrors();
      let isFocused = false;
      validationResult.errors?.forEach((error) => {
        const path = error.path.join(".");
        methods.setError(
          path,
          {
            type: "custom",
            message: error.message
          },
          { shouldFocus: !isFocused }
        );
        isFocused = true;
        const correctedPath = error.path?.slice?.(0, -1);
        if (correctedPath?.length > 0) {
          methods.setError(correctedPath.join("."), {
            type: "custom",
            message: error.message
          });
        }
      });
    }
  };
  return /* @__PURE__ */ (0, import_jsx_runtime4.jsx)(import_react_hook_form3.FormProvider, { ...methods, children: /* @__PURE__ */ (0, import_jsx_runtime4.jsx)(
    AutoFormProvider,
    {
      value: {
        schema: parsedSchema,
        uiComponents,
        formComponents
      },
      children: /* @__PURE__ */ (0, import_jsx_runtime4.jsxs)(
        uiComponents.Form,
        {
          onSubmit: methods.handleSubmit(handleSubmit),
          ...formProps,
          children: [
            parsedSchema.fields.map((field) => /* @__PURE__ */ (0, import_jsx_runtime4.jsx)(AutoFormField, { field, path: [field.key] }, field.key)),
            withSubmit && /* @__PURE__ */ (0, import_jsx_runtime4.jsx)(uiComponents.SubmitButton, { children: "Submit" }),
            children
          ]
        }
      )
    }
  ) });
}
