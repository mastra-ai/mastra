          case 2 /* PROJECT_LOGS */:
            return { project_id: this.objectId, log_id: "g" };
          default:
            throw new Error("Impossible");
        }
      }
      toObject() {
        return {
          objectType: this.objectType,
          objectId: this.objectId,
          rowIds: this.rowIds?.toObject()
        };
      }
    };
  }
});

