            this.rowIds.spanId
          );
          if (!spanIdIsUUID) {
            throw new Error("span_id component must be a valid UUID");
          }
          const { bytes: rootSpanIdBytes, isUUID: rootSpanIdIsUUID } = tryMakeUuid(
            this.rowIds.rootSpanId
          );
