        this.rowId = args.rowId;
        this.spanId = args.spanId;
        this.rootSpanId = args.rootSpanId;
        if (!this.rowId) {
          throw new Error("rowId must be nonempty string");
        }
        if (!this.spanId) {
          throw new Error("spanId must be nonempty string");
        }
