// src/v4/field-type-inference.ts
var import_v4 = require("zod/v4");
function inferFieldType(schema, fieldConfig) {
  if (fieldConfig?.fieldType) {
    return fieldConfig.fieldType;
  }
  if (schema instanceof import_v4.z.ZodObject) return "object";
  if (schema instanceof import_v4.z.ZodString) return "string";
  if (schema instanceof import_v4.z.ZodNumber) return "number";
  if (schema instanceof import_v4.z.ZodBoolean) return "boolean";
  if (schema instanceof import_v4.z.ZodDate) return "date";
  if (schema instanceof import_v4.z.ZodEnum) return "select";
  if (schema instanceof import_v4.z.ZodArray) return "array";
  return "string";
}

