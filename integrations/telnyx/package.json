{
  "name": "@mastra/telnyx",
  "version": "0.1.0",
  "description": "Telnyx telephony tool provider for Mastra agents",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:lib": "tsup --silent --config tsup.config.ts",
    "build:watch": "pnpm build:lib --watch",
    "lint": "eslint ."
  },
  "keywords": [
    "mastra",
    "telnyx",
    "sms",
    "voice",
    "telephony",
    "communication"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "integrations/telnyx"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "homepage": "https://mastra.ai",
  "engines": {
    "node": ">=22.13.0"
  },
  "dependencies": {
    "@mastra/core": "workspace:*"
  },
  "peerDependencies": {
    "telnyx": "^3.x"
  },
  "peerDependenciesMeta": {
    "telnyx": {
      "optional": true
    }
  },
  "devDependencies": {
    "@internal/lint": "workspace:*",
    "@internal/types-builder": "workspace:*",
    "telnyx": "^3.0.0",
    "tsup": "^8.5.1",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "^3.25.76"
  }
}
