{
  "name": "@mastra/core",
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/mylib.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "check": "tsc --noEmit",
    "analyze": "size-limit --why",
    "build": "dts build && cp ./src/prisma/* ./dist/prisma",
    "build:dev": "dts watch",
    "lint": "dts lint",
    "size": "size-limit",
    "start": "dts watch",
    "test": "jest",
    "clean": "rm -rf dist && rm -rf node_modules",
    "generate": "node --env-file .env ./src/prisma/gen.js && npx prisma generate --schema=./src/prisma/schema.prisma",
    "synchronize": "npx prisma db push --schema=./src/prisma/schema.prisma --force-reset",
    "postinstall": "pnpm generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@badgateway/oauth2-client": "^2.4.0",
    "@date-fns/utc": "^1.2.0",
    "@prisma/client": "^5.18.0",
    "date-fns": "^3.0.5",
    "inngest": "^3.22.3",
    "lodash": "^4.17.21",
    "next": "^14.2.5",
    "prisma": "^5.18.0",
    "qs": "^6.13.0"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=20 <22"
  },
  "size-limit": [
    {
      "path": "dist/mylib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/mylib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@badgateway/oauth2-client": "^2.4.0",
    "@jest/globals": "^29.7.0",
    "@size-limit/preset-small-lib": "^11.1.4",
    "@tsconfig/recommended": "^1.0.7",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.1.0",
    "@types/qs": "^6.9.15",
    "dts-cli": "^2.0.5",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "size-limit": "^11.1.4",
    "ts-jest": "^29.2.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  }
}
