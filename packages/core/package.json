{
  "name": "@mastra/core",
  "version": "0.2.0-alpha.85",
  "license": "MIT",
  "type": "module",
  "main": "dist/core.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.js"
      }
    },
    "./llm": {
      "import": {
        "types": "./dist/llm/model/index.d.ts",
        "default": "./dist/llm/model/index.js"
      }
    },
    "./llm/*": {
      "import": {
        "types": "./dist/llm/model/providers/*.d.ts",
        "default": "./dist/llm/model/providers/*.js"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils.d.ts",
        "default": "./dist/utils.js"
      }
    },
    "./base": {
      "import": {
        "types": "./dist/base.d.ts",
        "default": "./dist/base.js"
      }
    },
    "./action": null,
    "./queue": null,
    "./run": null,
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "check": "tsc --noEmit",
    "analyze": "size-limit --why",
    "build": "tsup-node src/index.ts src/base.ts src/utils.ts src/*/index.ts src/llm/model/providers/*.ts --format esm --clean --dts --treeshake",
    "build:dev": "pnpm build --watch",
    "size": "size-limit",
    "test": "vitest run"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^1.0.8",
    "@ai-sdk/anthropic": "^1.0.8",
    "@ai-sdk/azure": "^1.0.20",
    "@ai-sdk/cohere": "^1.0.8",
    "@ai-sdk/google": "^1.0.16",
    "@ai-sdk/mistral": "^1.0.8",
    "@ai-sdk/openai": "^1.0.18",
    "@ai-sdk/xai": "^1.0.17",
    "@badgateway/oauth2-client": "^2.4.0",
    "@date-fns/utc": "^1.2.0",
    "@libsql/client": "^0.14.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.53.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.55.0",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/sdk-metrics": "^1.28.0",
    "@opentelemetry/sdk-node": "^0.55.0",
    "@opentelemetry/sdk-trace-base": "^1.28.0",
    "@opentelemetry/sdk-trace-node": "^1.28.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@pinecone-database/pinecone": "^3.0.3",
    "@types/express": "^5.0.0",
    "@upstash/redis": "^1.34.3",
    "ai": "^4.0.34",
    "anthropic-vertex-ai": "^1.0.2",
    "cohere-ai": "^7.15.2",
    "date-fns": "^3.0.5",
    "express": "^4.21.1",
    "json-schema": "^0.4.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "radash": "^12.1.0",
    "sift": "^17.1.3",
    "voyage-ai-provider": "^1.0.0",
    "xstate": "^5.19.0",
    "zod": "^3.24.1"
  },
  "engines": {
    "node": ">=20"
  },
  "size-limit": [
    {
      "path": "dist/mylib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/mylib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@badgateway/oauth2-client": "^2.4.0",
    "@size-limit/preset-small-lib": "^11.1.4",
    "@tsconfig/recommended": "^1.0.7",
    "@types/json-schema": "^7.0.15",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.1.0",
    "@types/pino": "^7.0.5",
    "@types/qs": "^6.9.15",
    "size-limit": "^11.1.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  },
  "keywords": [
    "ai",
    "llm",
    "llms",
    "agent",
    "agents",
    "vectorstore",
    "embeddings",
    "rag",
    "evals",
    "memory",
    "tools",
    "telemetry",
    "typescript",
    "opentelemetry",
    "otel",
    "framework"
  ]
}
