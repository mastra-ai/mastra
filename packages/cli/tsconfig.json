{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "bundler",
    "outDir": "./dist",
    "rootDir": "./src",
    "noEmit": false,
    "jsx": "react-jsx",
    "strictNullChecks": false,
    "noImplicitReturns": false
  },
  "include": ["src/**/*", "postcss.config.js"],
  "exclude": [
    "node_modules",
    "src/starter-files/**",
    "src/playground/**",
    "**/*.test.ts",
    "src/homepage/dist",
    "src/chat/agent/dist"
  ]
}
