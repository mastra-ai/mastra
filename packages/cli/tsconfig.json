{
"compilerOptions": {
  "lib": ["dom", "dom.iterable", "esnext"],
  "allowJs": true,
  "skipLibCheck": true,
  "strict": true,
  "noEmit": true,
  "esModuleInterop": true,
  "module": "esnext",
  "moduleResolution": "bundler",
  "resolveJsonModule": true,
  "isolatedModules": true,
  "jsx": "preserve",
  "incremental": true,
  "plugins": [
    {
      "name": "next"
    }
  ],
  "paths": {
    "@/*": ["./src/*"]
  },
  "forceConsistentCasingInFileNames": true
},
"exclude": ["node_modules", "src/files/**"]
}