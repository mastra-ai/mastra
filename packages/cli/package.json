{
  "name": "mastra",
  "version": "0.1.57-alpha.99",
  "license": "MIT",
  "description": "cli for mastra",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "mastra": "./dist/index.js"
  },
  "files": [
    "dist",
    "src/starter-files",
    "src/scripts",
    "src/playground/dist",
    "node_modules"
  ],
  "scripts": {
    "build": "npm-run-all build:tsc copy-starter-files build:playground",
    "build:tsc": "tsc",
    "build:dev": "npx tsc --watch",
    "copy-starter-files": "cpy 'src/starter-files/**/*' dist/starter-files",
    "build:playground": "cd src/playground && pnpm build",
    "dev:playground": "cd src/playground && pnpm dev",
    "init": "npx tsx src/index.ts init",
    "dev": "npx tsx src/index.ts dev",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "clean": "rm -rf dist && rm -rf node_modules",
    "typecheck": "tsc --noEmit --incremental",
    "postinstall": "node src/scripts/postinstall.js"
  },
  "keywords": [
    "mastra",
    "cli",
    "ai"
  ],
  "author": "",
  "dependencies": {
    "@clack/prompts": "^0.8.2",
    "@dagrejs/dagre": "^1.1.4",
    "@mastra/core": "workspace:*",
    "@mastra/deployer": "workspace:*",
    "@swc/core": "^1.9.3",
    "@types/figlet": "^1.7.0",
    "chokidar": "^4.0.3",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.24.0",
    "execa": "^9.3.1",
    "express": "^4.21.1",
    "express-jsdoc-swagger": "^1.8.0",
    "figlet": "^1.8.0",
    "fs-extra": "^11.2.0",
    "is-port-reachable": "^4.0.0",
    "itty-router": "^5.0.18",
    "json-schema-to-zod": "^2.5.0",
    "memfs": "^4.14.0",
    "picocolors": "^1.1.1",
    "posthog-node": "^4.3.1",
    "prettier": "^3.3.3",
    "prompt": "^1.3.0",
    "serverless-http": "^3.1.1",
    "shiki": "^1.24.2",
    "superjson": "^2.2.2",
    "swr": "^2.2.5",
    "tcp-port-used": "^1.0.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^4.30.0",
    "typescript": "^5.5.4",
    "yocto-spinner": "^0.1.1",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.10.0",
    "@types/prompt": "^1.1.8",
    "@types/tcp-port-used": "^1.0.4",
    "cpy-cli": "^5.0.0",
    "vitest": "^2.1.8"
  }
}
