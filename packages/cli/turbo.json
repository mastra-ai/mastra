{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build:lib": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "inputs": ["$TURBO_DEFAULT$", "./tsup.config.ts"]
    },
    "build": {
      "dependsOn": ["^build", "build:lib", "@internal/playground#build"],
      "outputs": ["dist/**"]
    }
  }
}
