{
  "name": "@mastra/deployer",
  "version": "0.0.1-alpha.3",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/deployer.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/deployer.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./server": {
      "import": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild:app": "esbuild --platform=node --target=node20 --format=esm --bundle --outdir=dist/server src/server/index.ts",
    "build": "dts build && pnpm prebuild:app",
    "build:dev": "concurrently 'dts watch' 'sleep 5 && pnpm prebuild:app --watch'",
    "pull:openapispec": "node src/server/openapi.script.js",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@hono/swagger-ui": "^0.5.0",
    "@hono/zod-validator": "^0.4.2",
    "@mastra/core": "workspace:*",
    "dotenv": "^16.3.1",
    "esbuild": "^0.24.2",
    "execa": "^9.3.1",
    "fs-extra": "^11.2.0",
    "hono": "^4.6.17",
    "hono-openapi": "^0.4.3",
    "superjson": "^2.2.2",
    "type-fest": "^4.30.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@tsconfig/recommended": "^1.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "concurrently": "^9.1.0",
    "dts-cli": "^2.0.5",
    "vitest": "^2.1.8"
  }
}
