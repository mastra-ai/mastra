{
  "name": "@mastra/deployer",
  "version": "0.0.1-alpha.11",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "default": "./dist/server/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prebuild:app": "esbuild --platform=node --target=node20 --format=esm --bundle --outdir=dist/server src/server/index.ts",
    "build": "tsup-node src/index.ts src/server/index.ts --format esm --clean --dts --treeshake",
    "build:dev": "pnpm run build --watch",
    "pull:openapispec": "node src/server/openapi.script.js",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@hono/swagger-ui": "^0.5.0",
    "@mastra/core": "workspace:*",
    "esbuild": "^0.24.2",
    "execa": "^9.3.1",
    "fs-extra": "^11.2.0",
    "hono": "^4.6.17",
    "hono-openapi": "^0.4.3",
    "superjson": "^2.2.2",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.9.0",
    "concurrently": "^9.1.0",
    "tsup": "^8.3.6",
    "type-fest": "^4.30.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  }
}
