  "dependencies": {
    "@mastra/core": "workspace:*"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@tsconfig/recommended": "^1.0.7",
    "@types/node": "^22.9.0",
    "tsup": "^8.0.1",
    "vitest": "^3.0.4"
  }
