    "jsdom": "^26.0.0",
    "turndown": "^7.1.2",
    "tylerbarnes-fastmcp-fix": "^1.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^1.1.15",
    "@internal/lint": "workspace:*",
    "@mastra/astra": "workspace:^",
    "@mastra/chroma": "workspace:^",
    "@mastra/client-js": "workspace:^",
    "@mastra/composio": "workspace:^",
