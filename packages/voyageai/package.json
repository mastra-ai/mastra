{
  "name": "@mastra/voyageai",
  "version": "0.1.0-alpha.1",
  "description": "VoyageAI embeddings integration for Mastra - text, multimodal, and contextualized chunk embeddings",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "packages/voyageai"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "scripts": {
    "build": "tsup --silent --config tsup.config.ts",
    "build:watch": "tsup --watch --silent --config tsup.config.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "voyageai": "^0.1.0"
  },
  "peerDependencies": {
    "zod": "^3.25.0 || ^4.0.0"
  },
  "devDependencies": {
    "@internal/ai-sdk-v4": "workspace:*",
    "@internal/ai-sdk-v5": "workspace:*",
    "@internal/ai-v6": "workspace:*",
    "@internal/lint": "workspace:*",
    "@internal/types-builder": "workspace:*",
    "@types/node": "22.13.17",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "tsup": "^8.5.0",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "^3.25.76"
  },
  "engines": {
    "node": ">=22.13.0"
  },
  "keywords": [
    "mastra",
    "voyage",
    "voyageai",
    "embeddings",
    "multimodal",
    "ai",
    "rag",
    "vector"
  ]
}
