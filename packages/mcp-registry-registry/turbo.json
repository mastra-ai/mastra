{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "outputs": ["dist/**", "!dist/docs/**"],
      "dependsOn": ["^build"]
    },
    "build:cli": {
      "outputs": ["dist/**", "!dist/docs/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": ["build", "build:cli"],
      "outputs": []
    }
  }
}
