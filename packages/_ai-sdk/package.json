{
  "name": "@internal/ai-sdk",
  "version": "0.0.0",
  "description": "Internal vendor package for all AI SDK versions",
  "type": "module",
  "private": true,
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./v4": {
      "types": "./dist/v4/index.d.ts",
      "import": "./dist/v4/index.js",
      "default": "./dist/v4/index.js"
    },
    "./v4/*": {
      "types": "./dist/v4/*.d.ts",
      "import": "./dist/v4/*.js",
      "default": "./dist/v4/*.js"
    },
    "./v5": {
      "types": "./dist/v5/index.d.ts",
      "import": "./dist/v5/index.js",
      "default": "./dist/v5/index.js"
    },
    "./v5/*": {
      "types": "./dist/v5/*.d.ts",
      "import": "./dist/v5/*.js",
      "default": "./dist/v5/*.js"
    }
  },
  "scripts": {
    "build": "tsup --silent --config tsup.config.ts",
    "test": "vitest run",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@ai-sdk/provider": "^1.1.3",
    "@ai-sdk/provider-utils": "^2.2.8",
    "@ai-sdk/provider-v5": "npm:@ai-sdk/provider@2.0.0",
    "@ai-sdk/provider-utils-v5": "npm:@ai-sdk/provider-utils@3.0.12",
    "@internal/lint": "workspace:*",
    "@microsoft/api-extractor": "^7.53.1",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "ai": "^4.3.19",
    "ai-v5": "npm:ai@5.0.97",
    "eslint": "^9.36.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "zod": "^3.25.0"
  },
  "peerDependencies": {
    "zod": "^3.25.0 || ^4.0.0"
  },
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "packages/_ai-sdk"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.15"
  }
}
