{
  "name": "@mastra/client",
  "version": "0.0.1-alpha.0",
  "description": "Mastra Client SDK",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/client.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/client.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "dts build",
    "build:dev": "dts watch",
    "test": "vitest run",
    "gen:client": "npx @hey-api/openapi-ts -c @hey-api/client-fetch -i ./src/openapi.json -o ./src/generated"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hey-api/client-fetch": "^0.3.3",
    "@hey-api/openapi-ts": "^0.62.3",
    "@mastra/core": "workspace:*",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@tsconfig/recommended": "^1.0.7",
    "@types/node": "^22.9.0",
    "dts-cli": "^2.0.5",
    "vitest": "^2.1.8"
  }
}
