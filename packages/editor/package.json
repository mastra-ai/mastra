{
  "name": "@mastra/editor",
  "version": "0.1.0",
  "description": "Mastra Editor for agent management and instantiation",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "validate:package": "echo 'Validating package.json...'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "packages/editor"
  },
  "homepage": "https://mastra.ai",
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "dependencies": {
    "@mastra/memory": "workspace:*"
  },
  "devDependencies": {
    "@internal/ai-sdk-v4": "workspace:*",
    "@internal/ai-sdk-v5": "workspace:*",
    "@internal/ai-v6": "workspace:*",
    "@mastra/core": "workspace:*",
    "@mastra/libsql": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "@mastra/core": ">=1.0.0-0 <2.0.0-0",
    "zod": "^3.25.0 || ^4.0.0"
  },
  "engines": {
    "node": ">=22.13.0"
  }
}
