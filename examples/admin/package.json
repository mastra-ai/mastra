{
  "name": "examples-admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Demo application showcasing Mastra Admin functionality",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev": "npx tsx src/index.ts",
    "demo:teams": "npx tsx src/demo-teams.ts",
    "demo:projects": "npx tsx src/demo-projects.ts",
    "demo:full": "npx tsx src/demo-full.ts",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up -d"
  },
  "dependencies": {
    "@mastra/admin": "latest",
    "@mastra/admin-pg": "latest",
    "@mastra/admin-server": "latest",
    "@mastra/source-local": "latest",
    "@mastra/observability-writer": "latest",
    "dotenv": "^16.4.7"
  },
  "pnpm": {
    "overrides": {
      "@mastra/admin": "link:../../packages/admin",
      "@mastra/admin-pg": "link:../../stores/admin-pg",
      "@mastra/admin-server": "link:../../packages/admin-server",
      "@mastra/source-local": "link:../../sources/local",
      "@mastra/observability-writer": "link:../../observability/writer"
    }
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f"
}
