{
  "name": "@genstudio/stripped-agent-hub",
  "version": "0.1.0",
  "description": "GenStudio StrippedAgent Hub",
  "private": true,
  "type": "module",
  "scripts": {
    "tsc": "rm -rf dist && tsc -p tsconfig.build.json",
    "build": "npm run tsc",
    "lint": "prettier --check '*/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "prettier --write '*/**/*.{js,jsx,ts,tsx}'",
    "start:playground": "LOG_PRETTY=true NODE_CONFIG_ENV=playground mastra dev --dir src/core/mastra/playground",
    "start:dev": "OTEL_SERVICE_NAME=genstudio-agent-hub LOG_PRETTY=true OTEL_LOGS_EXPORTER=none NODE_CONFIG_ENV=localdev KNOWN_EXTS=js,ts nodemon -e ts,json --exec node -r dotenv/config --loader ts-node/esm --no-warnings=ExperimentalWarning --import=specifier-resolution-node/register --import=./instrumentation-hook.js --import=./src/core/telemetry/init.ts src/server.ts",
    "start": "OTEL_SERVICE_NAME=genstudio-agent-hub LOG_PRETTY=true OTEL_LOGS_EXPORTER=none NODE_CONFIG_ENV=localdev node -r dotenv/config --import=specifier-resolution-node/register --import=./instrumentation-hook.js --import=./dist/core/telemetry/init.js dist/server.js",
    "postgres:start": "make run-depsonly",
    "postgres:stop": "make stop-depsonly"
  },
  "license": "UNLICENSED",
  "engines": {
    "node": "^v20",
    "npm": "^10"
  },
  "dependencies": {
    "@ag-ui/mastra": "0.2.0",
    "@ai-sdk/azure": "1.3.25",
    "@ai-sdk/google-vertex": "2.2.27",
    "@ai-sdk/openai": "1.3.24",
    "@copilotkit/runtime": "1.10.5",
    "@fastify/cors": "11.1.0",
    "@fastify/otel": "0.9.4",
    "@google/genai": "1.19.0",
    "@libsql/client": "0.15.15",
    "@mastra/core": "workspace:*",
    "@mastra/libsql": "workspace:*",
    "@mastra/loggers": "workspace:*",
    "@mastra/memory": "workspace:*",
    "@mastra/observability": "workspace:*",
    "@mastra/otel-bridge": "workspace:*",
    "@mastra/server": "workspace:*",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.64.1",
    "@opentelemetry/host-metrics": "0.36.0",
    "@opentelemetry/sdk-node": "0.205.0",
    "@opentelemetry/semantic-conventions": "1.37.0",
    "async-mutex": "^0.5.0",
    "base64-arraybuffer": "^1.0.2",
    "config": "4.1.1",
    "dotenv": "17.2.0",
    "fastest-levenshtein": "1.0.16",
    "fastify": "5.6.0",
    "fastify-plugin": "5.0.1",
    "fastify-type-provider-zod": "4.0.2",
    "handlebars": "4.7.8",
    "hono": "4.9.7",
    "openai": "4.104.0",
    "p-retry": "7.1.0",
    "pino": "9.11.0",
    "specifier-resolution-node": "1.1.4",
    "uri-template": "2.0.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/config": "3.3.5",
    "@types/node": "20.19.7",
    "concurrently": "9.2.1",
    "mastra": "workspace:*",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  }
}
