{
  "name": "examples-unified-workspace",
  "type": "module",
  "private": true,
  "description": "Example demonstrating the unified Workspace API with filesystem, skills, and search",
  "version": "0.0.1",
  "dependencies": {
    "@ai-sdk/openai": "^3.0.0",
    "@ai-sdk/provider": "^3.0.0",
    "@mastra/core": "latest",
    "@mastra/e2b": "latest",
    "@mastra/s3": "latest",
    "@mastra/libsql": "latest",
    "@mastra/memory": "latest",
    "ai": "^6.0.1",
    "mastra": "latest",
    "tsx": "^4.19.0",
    "typescript": "^5.8.3",
    "zod": "^3"
  },
  "pnpm": {
    "overrides": {
      "@mastra/core": "link:../../packages/core",
      "@mastra/e2b": "link:../../workspaces/e2b",
      "@mastra/s3": "link:../../workspaces/s3",
      "@mastra/memory": "link:../../packages/memory",
      "@mastra/libsql": "link:../../stores/libsql",
      "mastra": "link:../../packages/cli",
      "@mastra/client-js": "link:../../client-sdks/client-js"
    }
  },
  "scripts": {
    "demo:skills": "npx bun src/demo/demo-skills.ts",
    "demo:workspace": "npx bun src/demo/demo-workspace.ts",
    "demo:filesystem": "npx bun src/demo/demo-filesystem.ts",
    "demo:sandbox": "npx bun src/demo/demo-sandbox.ts",
    "demo:agents": "npx bun src/demo/demo-agents.ts",
    "test:e2b": "npx tsx src/test-e2b-sandbox.ts",
    "agent:e2b": "npx tsx src/agent-with-e2b.ts",
    "mastra:build": "mastra build",
    "mastra:dev": "mastra dev"
  },
  "keywords": [],
