{
  "name": "@mastra/redis",
  "version": "1.0.0",
  "description": "Redis cache provider for Mastra - works with any Redis-compatible client (ioredis, node-redis, upstash, etc.)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm build:lib",
    "build:lib": "tsup --silent --config tsup.config.ts",
    "build:docs": "pnpx tsx ../../scripts/generate-package-docs.ts stores/redis",
    "build:watch": "pnpm build:lib --watch",
    "pretest": "docker compose up -d && sleep 2",
    "test": "vitest run",
    "posttest": "docker compose down -v",
    "lint": "eslint ."
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@internal/lint": "workspace:*",
    "@internal/types-builder": "workspace:*",
    "@mastra/core": "workspace:*",
    "@types/node": "22.19.7",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "^9.37.0",
    "ioredis": "^5.6.1",
    "tsup": "^8.5.1",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "@mastra/core": ">=1.0.5-0 <2.0.0-0"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "stores/redis"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "engines": {
    "node": ">=22.13.0"
  }
}
