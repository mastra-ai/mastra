{
  "name": "@mastra/voice-aws-nova-sonic",
  "version": "0.12.0-beta.0",
  "description": "Mastra AWS Nova 2 Sonic voice integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup --silent --config tsup.config.ts",
    "postbuild": "pnpx tsx ../../scripts/generate-package-docs.ts voice/aws-nova-sonic",
    "build:watch": "tsup --watch --silent --config tsup.config.ts",
    "test": "vitest run",
    "lint": "eslint ."
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.993.0",
    "@aws-sdk/credential-provider-node": "^3.972.10",
    "@smithy/node-http-handler": "^4.0.0",
    "@smithy/types": "^4.0.0",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@ai-sdk/openai-v5": "npm:@ai-sdk/openai@2.0.69",
    "@internal/lint": "workspace:*",
    "@internal/types-builder": "workspace:*",
    "@mastra/core": "workspace:*",
    "@types/node": "22.13.17",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "catalog:",
    "@vitest/ui": "catalog:",
    "eslint": "^9.37.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "catalog:",
    "zod": "^3.25.76"
  },
  "peerDependencies": {
    "@mastra/core": ">=1.0.0-0 <2.0.0-0",
    "zod": "^3.25.0 || ^4.0.0"
  },
  "homepage": "https://mastra.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mastra-ai/mastra.git",
    "directory": "voice/aws-nova-sonic"
  },
  "bugs": {
    "url": "https://github.com/mastra-ai/mastra/issues"
  },
  "engines": {
    "node": ">=22.13.0"
  }
